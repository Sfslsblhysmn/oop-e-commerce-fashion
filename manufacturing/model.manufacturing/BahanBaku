package model.manufacturing;

public class BahanBaku {
    private int id;
    private String nama;
    private int stok;
    private String satuan;

public BahanBaku(int id, String nama, int stok, String satuan) {
    this.id = id;
    this.nama = nama;
    this.stok = stok;
    this.satuan = satuan;
}

public void kurangiStok(int jumlah) {
    this.stok -= jumlah;
}

public int getId() {
    return id;
}

public String getNama() {
    return nama;
}

public int getStok() {
    return stok;
}

public String getSatuan() {
    return satuan;
}
}
